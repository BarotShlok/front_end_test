{
  "compilerOptions": {
    "target": "ES6",
    "module": "commonjs",
    "jsx": "react",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"],
      "@service/*": ["src/service/*"] // Ensure this path is accurate
    }
  },
  "include": ["src/**/*", "__tests__/**/*"]
}
